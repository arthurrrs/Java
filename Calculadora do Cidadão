    private void bntCalcularActionPerformed(java.awt.event.ActionEvent evt) {                                            
       
       Double numeroMeses = Double.parseDouble(txtNumeroMeses.getText());
       Double taxaJurosMensal = Double.parseDouble(txtTaxaJuros.getText());
       Double valorDepósitoRegular = Double.parseDouble(txtDepósitoRegular.getText());
       Double valorObtidoFinal;
       
       Calculadora oper = new Calculadora();
       valorObtidoFinal = oper.calcAplicDepositosReg(numeroMeses, taxaJurosMensal,valorDepósitoRegular);
       
       txtValorPeriodo.setText(String.valueOf(valorObtidoFinal)); 
    }                                           

    private void bntLimparActionPerformed(java.awt.event.ActionEvent evt) {                                          
        txtNumeroMeses.setText("");
        txtTaxaJuros.setText("");
        txtValorPeriodo.setText("");
        txtDepósitoRegular.setText(""); 
      
    }                                         

